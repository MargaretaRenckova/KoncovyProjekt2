<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/purple_700</item>
        <item name="colorAccent">@color/black</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="molle">
        <item name="android:fontFamily">@font/molle_italic</item>
    </style>
    <style name="dancing">
        <item name="android:fontFamily">@font/dancing_script</item>
    </style>
    <style name="deltagothic">
        <item name="android:fontFamily">@font/delta_gothic</item>
    </style>
    <style name="covidnazov">
        <item name="android:fontFamily">@font/montserrat_toolbar</item>
    </style>
    <style name="montserrat_text">
        <item name="android:fontFamily">@font/montserrat_thin</item>
    </style>
    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorAccent">@color/siva2</item>
        <item name="colorPrimary">@color/white</item>
    </style>
    <style name="DialogTheme" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="android:textColorPrimary">#61211E</item>
    </style>
</resources>